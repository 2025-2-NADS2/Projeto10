    // Volunteer form submit (example: replace with endpoint)
    function submitVol(e){
      e.preventDefault();
      const name = document.getElementById('v-name').value.trim();
      const whats = document.getElementById('v-whats').value.trim();
      const place = document.getElementById('v-place').value.trim();
      const avail = document.getElementById('v-avail').value.trim();
      const msg = document.getElementById('v-msg').value.trim();
      // Basic validation
      if(!name || !whats){ alert('Preencha nome e WhatsApp'); return; }
      // Here you should POST to your server / Zapier / Formspree / Google Sheets
      console.log({name,whats,place,avail,msg});
      alert('Obrigado, ' + name + '! Recebemos sua inscri√ß√£o. Em breve entraremos em contato pelo WhatsApp.');
      // reset
      document.getElementById('volForm').reset();
    }

    // Small helper for external links
    (function enhanceLinks(){
      const links = document.querySelectorAll('a[href^="#"]');
      links.forEach(a => a.addEventListener('click', (e)=>{
        const href = a.getAttribute('href');
        if(href === '#') e.preventDefault();
      }));
    })();
  </script>
